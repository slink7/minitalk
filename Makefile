
#===CONFIGURATION===
NAME_SERVER = server
NAME_CLIENT = client

SRC_SERVER =\
	main_server.c\
	t_strbuilder.c\
	set_sig.c\

SRC_CLIENT =\
	main_client.c\
	set_sig.c\

CFLAGS = -Wall -Werror -Wextra

LIBS = \
	libft/libft.a\

INCLUDES = -Ilibft/

OBJ_DIR = obj

#===AUTOMATIC VARS===

OBJ_SERVER = $(addprefix $(OBJ_DIR)/, $(addsuffix .o, $(notdir $(basename $(SRC_SERVER)))))
OBJ_CLIENT = $(addprefix $(OBJ_DIR)/, $(addsuffix .o, $(notdir $(basename $(SRC_CLIENT)))))

LIB_FLAGS = $(addprefix -L, $(dir $(LIBS))) $(addprefix -l, $(patsubst lib%.a, %, $(notdir $(LIBS))))

#===TARGETS===
all : $(NAME_SERVER) $(NAME_CLIENT)

#===COMPILING===
$(OBJ_DIR) :
	$(shell mkdir -p $(OBJ_DIR))
$(OBJ_DIR)/%.o : %.c
	cc $(CFLAGS) -o $@ -c $< $(INCLUDES)
$(OBJ_DIR)/%.o : %.c
	cc $(CFLAGS) -o $@ -c $< $(INCLUDES)
%.a :
	make -C $(dir $@)

#===LINKING===
$(NAME_CLIENT) : $(OBJ_DIR) $(LIBS) $(OBJ_CLIENT)
	cc -o $(NAME_CLIENT) $(OBJ_CLIENT) $(LIB_FLAGS)
$(NAME_SERVER) : $(OBJ_DIR) $(LIBS) $(OBJ_SERVER)
	cc -o $(NAME_SERVER) $(OBJ_SERVER) $(LIB_FLAGS)

#===CLEAN===
clean :
	rm -rf $(OBJ_DIR) || true

#===FCLEAN===
fclean : clean
	rm -f $(NAME_SERVER) $(NAME_CLIENT) || true

#===RE===
re : fclean all


.PHONY : re fclean clean all default bonus

#1 * 100 : 0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789
